class Scene:
    def __init__(self):
        self.say1 = Customer()
        self.say2 = Clerk()
        self.say3 = Parrot()
    def action(self):
        print(self.say1.say())
        print(self.say2.say())
        print(self.say3.say())
class Customer:
    def say(self):
        return "Customer: \"Text\""
class Clerk:
    def say(self):
        return("Clerk: \"Text\"")
class Parrot:
    def say(self):
        return("Parrot: \"Text\"")


Scene().action()
